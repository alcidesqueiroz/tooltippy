{
  "brackets": "always",
  "colons": "always",
  "colors": "always",
  "commaSpace": "always",
  "commentSpace": "always",
  "customProperties": [],
  "depthLimit": 4,
  "duplicates": false,
  "efficient": "always",
  "exclude": ["playground/**/*", "node_modules/**/*"],
  "groupOutputByFile": true,
  "indentPref": 2,
  "leadingZero": "always",
  "mixed": true,
  "namingConvention": "BEM",
  "namingConventionStrict": true,
  "noImportant": true,
  "parenSpace": "never",
  "placeholders": "always",
  "prefixVarsWithDollar": "never",
  "quotePref": "single",
  "semicolons": "always",
  "valid": true,
  "zeroUnits": "never"
}
